<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CalculatorTheGameSolverApp.Views.SingleItemView"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:CalculatorTheGameSolverApp.ViewModels;assembly=CalculatorTheGameSolverApp"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance viewModels:AvailableOperationVM}">
  <ContentView.Content>
        <StackLayout>
            <Entry Placeholder="value" Text="{Binding Path=Value1, Mode=TwoWay}"/>
        </StackLayout>
    </ContentView.Content>
</ContentView>